{
    "name": "shopwarelabs/sw-cli-tools",
    "description": "A CLI toolset for shopware",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=5.3.2",
        "ext-curl": "*",
        "symfony/console": "~2.5",
        "symfony/config": "~2.5",
        "symfony/yaml": "~2.5",
        "symfony/process": "~2.5",
        "symfony/dependency-injection": "~2.5",
        "dnoegel/php-xdg-base-dir": "~0.1",
        "shopware-ag/plugin-info": "dev-master",
        "kherge/amend": "3.*",
        "guzzlehttp/guzzle": "^6.0",
        "piwik/ini": "^1.0"
    },
    "require-dev": {
        "ext-phar": "*",
        "phpunit/phpunit": "@stable",
        "squizlabs/php_codesniffer": "~2"
    },
    "autoload": {
        "psr-4": {
            "ShopwareCli\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ShopwareCli\\": "src/",
            "Shopware\\": "src/Extensions/Shopware"
        }
    },
    "bin": ["bin/sw"],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
